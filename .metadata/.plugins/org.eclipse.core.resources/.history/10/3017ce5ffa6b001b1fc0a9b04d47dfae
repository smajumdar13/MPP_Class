package Prob2b;

public class GradeReport {
	private Student student;
	
	public void addReport(GradeReport grade) {
		grade = (GradeReport) gr;
		student.setReport("A");
	}

	GradeReport(Student st) {
		this.student = st;
	}
	
	public Student getStudent() {
		return student;
	}
	
	public String getGrade() {
		return grade;
	}
}