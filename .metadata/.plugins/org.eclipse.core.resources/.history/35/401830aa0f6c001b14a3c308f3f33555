package Prob2b;

public class Student {
	private String name;
	private GradeReport report;
	Student(String name, GradeReport report) {
		this.name = name;
		this.report = report;	
	}
	public String getName() {
		return name;
	}
	public GradeReport getGrades() {
		return report;
	}

	@Override
	public String toString() {
		return name + " " + report;
	}
}