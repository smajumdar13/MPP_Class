package Prob2B;

import java.util.ArrayList;
import java.util.List;

public class Order {
	private String orderName;
	private List<OrderLine> item = new ArrayList<>();
		
	public void addItem(OrderLine itemName) {
		List<OrderLine> item = new ArrayList<OrderLine>();
		item.add(itemName);
	}
	//package level
	Order(String name) {
		this.orderName = name;
	}
	public List<OrderLine> getList() {
		return item;
	}
	public String getOrderName() {
		return orderName;
	}
	
	@Override
	public String toString() {
		String s = orderName;
		String n = "";
		for(OrderLine o: item) {
			n = o +"\n";
		}
		return s + n;
	}
}