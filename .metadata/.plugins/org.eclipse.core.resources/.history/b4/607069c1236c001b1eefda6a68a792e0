package Prob3d;

import java.util.ArrayList;
import java.util.List;

public class Main extends Property{

	public static void main(String[] args) {
		List<Property> lst = new ArrayList<Property>();
		
		lst.add(new House(10000, new Address("Str1", "Ct1", "St1", 11335)));
		lst.add(new Condominium(3, new Address("Str2", "Ct2", "St2", 12343)));
		lst.add(new Trailer(new Address("Str3", "Ct3", "St3", 12345)));
		
		
		for(Property p: lst) {
			System.out.println(p);
		}
		
		lst.to

	}

}
