package Prob2b;

//Customer "owns" the 1-1 relationship with ShoppingCart
public class Customer {
	private String name;
	private GradeReport cart;
	public Customer(String name) {
		this.name = name;
		cart = new GradeReport(this);		
	}
	public String getName() {
		return name;
	}
	public GradeReport getCart() {
		return cart;
	}
	
	@Override
	public String toString() {
		return name;
	}
}
