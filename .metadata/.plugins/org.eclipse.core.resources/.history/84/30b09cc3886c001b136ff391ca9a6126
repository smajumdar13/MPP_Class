package edu.miu.cs.cs401.quiz2.bankingapp;

import java.time.LocalDate;

public class Main {

	public static void main(String[] args) {
		Person p1 = new Person("C19102", "Anna Lynn Smith", LocalDate.of(1977, 1, 31), new Account(2801011119L, 10095.50));
		Person p2 = new Person("C24499", "Bob Jones", LocalDate.of(1989, 12, 7), new Account(2911111901L, 5590.35));
		
		Person[] p = new Person[2];
		p[0] = p1;
		p[1] = p2;
		
		for(Person per: p) {
			System.out.println(per);
		}
	}
}
