package Prob2b;
import java.util.*;

public class GradeReport {
	private Student owner;
	private List<Grade> lst = new ArrayList<>();
	private Grade g = new Grade();
	
	public void addGrade(Grade gr) {
		lst.add(gr);
	}
	//package level
	GradeReport(Student st) {
		owner = st;
	}
	public List<Grade> getGrades() {
		return g;
	}
	public Student getOwner() {
		return owner;
	}
}