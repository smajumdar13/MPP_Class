package Prob3d;

import java.util.ArrayList;
import java.util.List;
import Prob3d.Address;
import Prob3d.Condominium;
import Prob3d.House;
import Prob3d.Trailer;
import Prob3d.Property;

public class Main {

	public static void main(String[] args) {
	
//		lst.add(new House(10000, new Address("Str1", "Ct1", "St1", 11335)));
//		lst.add(new Condominium(3, new Address("Str2", "Ct2", "St2", 12343)));
//		lst.add(new Trailer(new Address("Str3", "Ct3", "St3", 12345)));
		
		Property p1 = new Trailer(new Address("Str1", "Ct1", "St1", 11335));
		Property p2 = new House(10000, new Address("Str2", "Ct2", "St2", 11355));
		Property p3 = new Condominium(3, new Address("Str3", "Ct3", "St3", 12343));

		Property[] p = new Property[3];
		p[0] = p1;
		p[1] = p2;
		p[2] = p3;
		
		for(Property pro: p) {
			System.out.println(pro);
		}
		
		
		
	}

}
