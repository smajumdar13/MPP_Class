package Prob3d;

public class Trailer extends Property {
	private double lotSize;
	
	public Trailer(double rent, Address address) {
		super(rent, address);
	}
	
	@Override
	double computeRent() {
		return getRent()*lotSize;
	}
	
	@Override
	public String toString() {
		return Double.toString(computeRent());
	}
}