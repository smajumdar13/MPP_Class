package Prob2b;
import java.util.*;

public class GradeReport {
	private Student owner;
	private List<Grade> g = new ArrayList<>();;
	public void addItem(Grade gr) {
		g.add(gr);
	}
	//package level
	GradeReport(Student st) {
		owner = st;
	}
	public List<Grade> getGrades() {
		return grades;
	}
	public Student getOwner() {
		return owner;
	}
}