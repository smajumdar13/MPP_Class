package Prob3d;

public class House extends Property {
	private double lotSize;
	
	public House(double rent, Address address) {
		super(rent, address);
	}

	public double getLotSize() {
		return lotSize;
	}
	
	public void setLotSize(double lotSize) {
		this.lotSize = lotSize;
	}
	
	@Override
	double computeRent() {
		rent = 0.1*lotSize;
		return rent;
	}
	
	
	
}
