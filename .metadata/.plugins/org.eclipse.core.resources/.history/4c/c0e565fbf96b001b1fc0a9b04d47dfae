package Prob2b;

public class GradeReport {
	private Student student;
	private String grade;
	
	public void addReport(String grade) {
		this.grade = grade;
	}

	GradeReport(Student st) {
		this.student = st;
	}
	
	public Student getStudent() {
		return student;
	}
	
	public String getGrade() {
		return grade;
	}
}