package Prob3b;

public class Apartment {
	private Building building;
	private Double rent;
	
	public Apartment(Double rent) {
		this.rent = rent;
	}

	public Double getRent() {
		return rent;
	}

	public void setRent(Double rent) {
		this.rent = rent;
	}
	
	public Building getBuilding() {
		return building;
	}
}
