package Prob2b;

public class GradeReport {
	private Student student;
	private Grade grade;
	
	public void addReport(String report) {
		this.report = report;
	}

	GradeReport(Student st) {
		this.student = st;
	}
	
	public Student getStudent() {
		return student;
	}
	
	public Grade getGrade() {
		return grade;
	}
}
