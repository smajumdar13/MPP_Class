package Prob3d;

public class Address {
	private String street;
	private String city;
	private String state;
	private String zip;
	
	public Address(String str, String ct, String st, String zip) {
		this.street = str;
		this.city = ct;
		this.state = st;
		this.zip = zip;
	}
	
	public String getStreet() {
		return street;
	}
	
	public String getCity() {
		return city;
	}
	
	public String getState() {
		return state;
	}
	
	public String getZip() {
		return zip;
	}
	
	public String toString() {
		return street + " " + city + " " + state + " " + zip;
	}
}