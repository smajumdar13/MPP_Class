package Prob2b;

public class Student {
	private String name;
	private GradeReport report = new GradeReport();
	public Student(String name) {
		this.name = name;
		report = new GradeReport(this);		
	}
	public String getName() {
		return name;
	}
	public GradeReport getGrades() {
		return report;
	}

	@Override
	public String toString() {
		return name;
	}
}